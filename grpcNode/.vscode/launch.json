            "request": "launch",
            "name": "Launch exampleServer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\exampleServer.js"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch static helloServer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\hello_server_static.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch static helloClient",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\hello_client_static.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch dynamic helloServer",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\hello_server_dynamic.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch dynamic helloClient",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\hello_client_dynamic.js"
